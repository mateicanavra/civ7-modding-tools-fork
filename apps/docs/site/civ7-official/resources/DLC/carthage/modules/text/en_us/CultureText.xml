<?xml version="1.0" encoding="utf-8"?>
<Database>
	<EnglishText>

		<!-- ==================================================	-->
		<!--	CARTHAGE							-->
		<!-- ==================================================	-->
		<Row Tag="LOC_TREE_CIVICS_AQ_CARTHAGE_NAME">
			<Text>Carthage Civics</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SHIPSHEDS_NAME">
			<Text>Shipsheds</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SHIPSHEDS_QUOTE">
			<Text>"Sailing quickly forth, we passed a country burning with fires and fragrance, from which streams of flames fell into the sea."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SHIPSHEDS_AUTHOR">
			<Text>Hanno the Navigator</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SICILIAN_WARS_NAME">
			<Text>Sicilian Wars</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SICILIAN_WARS_QUOTE">
			<Text>"Presenting the captured fleet, the Carthaginians ensured that the people could not only hear but see the greatness of their success."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SICILIAN_WARS_AUTHOR">
			<Text>Diodorus Siculus</Text>
		</Row>
		<Row Tag="LOC_CIVIC_RED_SLIP_NAME">
			<Text>Wisdom of Tanit</Text>
		</Row>
		<Row Tag="LOC_CIVIC_RED_SLIP_QUOTE">
			<Text>"He who finds more joy in an urban dwelling will have no need for a rural farm."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_RED_SLIP_AUTHOR">
			<Text>Mago</Text>
		</Row>
		<!--Carthage Civic Description-->
		<Row Tag="LOC_MOD_SHIPSHEDS_NAVAL_MOVEMENT_DESCRIPTION">
			<Text>+1 Movement to Naval Units</Text>
		</Row>
		<Row Tag="LOC_MOD_SHIPSHEDS_NAVAL_RANGE_DESCRIPTION">
			<Text>+1 Range to Naval Units</Text>
		</Row>
		<Row Tag="LOC_MOD_RED_SLIP_TRADE_GOLD_DESCRIPTION">
			<Text>+15 Sea Trade Route Range. +3 [icon:YIELD_GOLD] Gold in the Capital for every Trade Route from the Capital. </Text>
		</Row>
		<Row Tag="LOC_MOD_RED_SLIP_RESOURCE_CAP_DESCRIPTION">
			<Text>Increases the number of Resources that may be assigned to the Capital by 3.</Text>
		</Row>
		<Row Tag="LOC_MOD_SICILIAN_WARS_SETTLEMENT_CAP_DESCRIPTION">
			<Text>+2 Settlement Limit.</Text>
		</Row>
		<Row Tag="LOC_MOD_SICILIAN_WARS_MOVEMENT_NUMIDIAN_CAVALRY_DESCRIPTION">
			<Text>+1 Movement and Sight to Numidian Cavalry Units</Text>
		</Row>
		<Row Tag="LOC_MOD_SICILIAN_WARS_TOWN_PROJECT_DESCRIPTION">
			<Text>+1 Codex slot in City Halls in Towns with the Trade Outpost Focus.</Text>
		</Row>
		<!--Carthage Unique Traditions-->
		<Row Tag="LOC_TRADITION_GAULOS_NAME">
			<Text>Gaulos</Text>
		</Row>
		<Row Tag="LOC_TRADITION_GAULOS_DESCRIPTION">
			<Text>+25% [icon:YIELD_GOLD] Gold towards purchasing Buildings on Coast or Navigable Rivers.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_QUINQUEREME_NAME">
			<Text>Quinquereme</Text>
		</Row>
		<Row Tag="LOC_TRADITION_QUINQUEREME_DESCRIPTION">
			<Text>+25% [icon:YIELD_GOLD] Gold towards purchasing Naval Units. -1 [icon:YIELD_GOLD] Gold maintenance for Naval Units.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_SUFFETES_NAME">
			<Text>Suffetes</Text>
		</Row>
		<Row Tag="LOC_TRADITION_SUFFETES_DESCRIPTION">
			<Text>+20% [icon:YIELD_GOLD] Gold from Mining Towns. +20% [icon:YIELD_FOOD] Food from Fishing or Farming Towns.</Text>
		</Row>
	</EnglishText>
</Database>