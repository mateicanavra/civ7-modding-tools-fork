<?xml version="1.0" encoding="utf-8"?>
<Database>
	<EnglishText>
		<!-- ==================================================	-->
		<!--	MAIN TREE							-->
		<!-- ==================================================	-->
		<Row Tag="LOC_CIVIC_CHIEFDOM_NAME">
			<Text>Chiefdom</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CHIEFDOM_QUOTE">
			<Text>"I protect thee with this flame, and I drive away the foe and the sand from thy feet."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CHIEFDOM_QUOTE_AUTHOR">
			<Text>The Book of the Dead</Text>
		</Row>
		<Row Tag="LOC_CIVIC_MYSTICISM_NAME">
			<Text>Mysticism</Text>
		</Row>
		<Row Tag="LOC_CIVIC_MYSTICISM_QUOTE">
			<Text>"There is another world, but it is in this one."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_MYSTICISM_QUOTE_AUTHOR">
			<Text>Paul Éluard</Text>
		</Row>
		<Row Tag="LOC_CIVIC_DISCIPLINE_NAME">
			<Text>Discipline</Text>
		</Row>
		<Row Tag="LOC_CIVIC_DISCIPLINE_QUOTE">
			<Text>"Do nothing which is of no use."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_DISCIPLINE_QUOTE_AUTHOR">
			<Text>Miyamoto Musashi</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CITIZENSHIP_NAME">
			<Text>Citizenship</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CITIZENSHIP_QUOTE">
			<Text>"After the assembly gathered, the elders gave their answer."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CITIZENSHIP_QUOTE_AUTHOR">
			<Text>The Epic of Gilgamesh</Text>
		</Row>
		<Row Tag="LOC_CIVIC_PHILOSOPHY_NAME">
			<Text>Philosophy</Text>
		</Row>
		<Row Tag="LOC_CIVIC_PHILOSOPHY_QUOTE">
			<Text>"The unexamined life is not worth living."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_PHILOSOPHY_QUOTE_AUTHOR">
			<Text>Socrates</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CODE_OF_LAWS_NAME">
			<Text>Code of Laws</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CODE_OF_LAWS_QUOTE">
			<Text>"An eye for an eye, a tooth for a tooth."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CODE_OF_LAWS_QUOTE_AUTHOR">
			<Text>Exodus 21:24</Text>
		</Row>
		<Row Tag="LOC_CIVIC_LITERACY_NAME">
			<Text>Literacy</Text>
		</Row>
		<Row Tag="LOC_CIVIC_LITERACY_QUOTE">
			<Text>"God, with such splendid irony, granted me books and night at one touch."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_LITERACY_QUOTE_AUTHOR">
			<Text>Jorge Luis Borges</Text>
		</Row>
		<Row Tag="LOC_CIVIC_BANNERS_NAME">
			<Text>Tactics</Text>
		</Row>
		<Row Tag="LOC_CIVIC_BANNERS_QUOTE">
			<Text>"Those who delight to attack and to display their valour and fealty should be formed in companies."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_BANNERS_QUOTE_AUTHOR">
			<Text>Sun Tzu</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ORG_MILITARY_NAME">
			<Text>Organized Military</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ORG_MILITARY_QUOTE">
			<Text>"An army is a miniature of the society which produces it."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ORG_MILITARY_QUOTE_AUTHOR">
			<Text>C. L. R. James</Text>
		</Row>
		<Row Tag="LOC_CIVIC_COMMERCE_NAME">
			<Text>Commerce</Text>
		</Row>
		<Row Tag="LOC_CIVIC_COMMERCE_QUOTE">
			<Text>"The dogs bark, but the caravan moves on."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_COMMERCE_QUOTE_AUTHOR">
			<Text>Turkish Proverb</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SKILLED_TRADES_NAME">
			<Text>Skilled Trades</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SKILLED_TRADES_QUOTE">
			<Text>"Skill is handed down to me from my ancestors in a perpetual line of accumulated wisdom."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SKILLED_TRADES_QUOTE_AUTHOR">
			<Text>Shuji Nakagawa</Text>
		</Row>
		<Row Tag="LOC_CIVIC_PUBLIC_LIFE_NAME">
			<Text>Public Life</Text>
		</Row>
		<Row Tag="LOC_CIVIC_PUBLIC_LIFE_QUOTE">
			<Text>"Remain less interested in who we imagine ourselves to be than in what we can do for one another."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_PUBLIC_LIFE_QUOTE_AUTHOR">
			<Text>Paul Gilroy</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ENTERTAINMENT_NAME">
			<Text>Entertainment</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ENTERTAINMENT_QUOTE">
			<Text>"All the world's a stage, and all the men and women merely players."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ENTERTAINMENT_QUOTE_AUTHOR">
			<Text>William Shakespeare</Text>
		</Row>
		<Row Tag="LOC_CIVIC_FUTURE_CIVIC_AQ_NAME">
			<Text>Future Civic</Text>
		</Row>
		<Row Tag="LOC_CIVIC_FUTURE_CIVIC_AQ_QUOTE">
			<Text></Text>
		</Row>
		<Row Tag="LOC_CIVIC_FUTURE_CIVIC_AQ_AUTHOR">
			<Text></Text>
		</Row>
		<Row Tag="LOC_MOD_AQ_FUTURE_CIVIC_ATTRIBUTE_DESCRIPTION">
			<Text>+1 Random Attribute Point.</Text>
		</Row>
		<Row Tag="LOC_MOD_FUTURE_CIVIC_BOOST_DESCRIPTION">
			<Text>A boost for a non-Civilization-unique starting Civic in the next age.</Text>
		</Row>
		<Row Tag="LOC_MOD_AQ_FUTURE_CIVIC_AGE_PROGRESS_DESCRIPTION">
			<Text>+10 Age Progress.</Text>
		</Row>
		<!-- Aksum Unique Tree -->
		<Row Tag="LOC_TREE_CIVICS_AQ_AKSUM_NAME">
			<Text>Aksumite Civics</Text>
		</Row>
		<Row Tag="LOC_CIVIC_PERIPLUS_OF_THE_ERYTHRAEAN_SEA_NAME">
			<Text>Periplus of the Erythraean Sea</Text>
		</Row>
		<Row Tag="LOC_CIVIC_PERIPLUS_OF_THE_ERYTHRAEAN_SEA_QUOTE">
			<Text>"To that place all the ivory is brought."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_PERIPLUS_OF_THE_ERYTHRAEAN_SEA_QUOTE_AUTHOR">
			<Text>The Periplus of the Erythraean Sea</Text>
		</Row>
		<Row Tag="LOC_CIVIC_MONUMENTUM_ADULITANUM_NAME">
			<Text>Monumentum Adulitanum</Text>
		</Row>
		<Row Tag="LOC_CIVIC_MONUMENTUM_ADULITANUM_QUOTE">
			<Text>"I set up here this throne and consecrated it to Ares."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_MONUMENTUM_ADULITANUM_QUOTE_AUTHOR">
			<Text>Monumentum Adulitanum</Text>
		</Row>
		<Row Tag="LOC_CIVIC_BOOK_OF_THE_HIMYARITES_NAME">
			<Text>Book of the Himyarites</Text>
		</Row>
		<Row Tag="LOC_CIVIC_BOOK_OF_THE_HIMYARITES_QUOTE">
			<Text>"Africa will write its own history, and in both north and south it will be a history of glory and dignity."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_BOOK_OF_THE_HIMYARITES_QUOTE_AUTHOR">
			<Text>Patrice Lumumba</Text>
		</Row>
		<!-- Egypt Unique Tree -->
		<Row Tag="LOC_TREE_CIVICS_AQ_EGYPT_NAME">
			<Text>Egyptian Civics</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ARRIVAL_OF_HAPI_NAME">
			<Text>Arrival of Hapi</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ARRIVAL_OF_HAPI_QUOTE">
			<Text>"The river begins to rise when other rivers diminish and begins to subside just when other rivers increase."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ARRIVAL_OF_HAPI_QUOTE_AUTHOR">
			<Text>Ibn Battuta</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SCALES_OF_ANUBIS_NAME">
			<Text>Scales of Anubis</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SCALES_OF_ANUBIS_QUOTE">
			<Text>"The god Anubis said: Pass, thou, for thou knowest the names."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SCALES_OF_ANUBIS_QUOTE_AUTHOR">
			<Text>The Book of the Dead</Text>
		</Row>
		<Row Tag="LOC_CIVIC_LIGHT_OF_AMUN_RA_NAME">
			<Text>Light of Amun-Ra</Text>
		</Row>
		<Row Tag="LOC_CIVIC_LIGHT_OF_AMUN_RA_QUOTE">
			<Text>"Behold, thou image of gold, thou lord of terror; thou rollest along and art renewed day by day."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_LIGHT_OF_AMUN_RA_QUOTE_AUTHOR">
			<Text>The Book of the Dead</Text>
		</Row>
		<!-- Greece Unique Tree -->
		<Row Tag="LOC_TREE_CIVICS_AQ_GREECE_NAME">
			<Text>Greek Civics</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CLASSICAL_ORDERS_NAME">
			<Text>Classical Orders</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CLASSICAL_ORDERS_QUOTE">
			<Text>"The architect must not only understand drawing, but music."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CLASSICAL_ORDERS_QUOTE_AUTHOR">
			<Text>Vitruvius</Text>
		</Row>
		<Row Tag="LOC_CIVIC_AGOGE_NAME">
			<Text>Agoge</Text>
		</Row>
		<Row Tag="LOC_CIVIC_AGOGE_QUOTE">
			<Text>"If you think that I rely on numbers, then all Greece is not sufficient. If on men's valor, then this number will do."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_AGOGE_QUOTE_AUTHOR">
			<Text>Leonidas</Text>
		</Row>
		<Row Tag="LOC_CIVIC_EKKLESIA_NAME">
			<Text>Ekklesia</Text>
		</Row>
		<Row Tag="LOC_CIVIC_EKKLESIA_QUOTE">
			<Text>"The administration is in the hands of the many and not of the few."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_EKKLESIA_QUOTE_AUTHOR">
			<Text>Pericles</Text>
		</Row>
		<Row Tag="LOC_CIVIC_THALASSOCRACY_NAME">
			<Text>Thalassocracy</Text>
		</Row>
		<Row Tag="LOC_CIVIC_THALASSOCRACY_QUOTE">
			<Text>"Through the music of the languid hours, they hear like Ocean on a western beach the surge and thunder of the Odyssey."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_THALASSOCRACY_QUOTE_AUTHOR">
			<Text>Andrew Lang</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SYMMACHIA_NAME">
			<Text>Symmachia</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SYMMACHIA_QUOTE">
			<Text>"Right, as the world goes, is only in question between equals in power, while the strong do what they can and the weak suffer what they must."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SYMMACHIA_QUOTE_AUTHOR">
			<Text>Thucydides</Text>
		</Row>
		<!-- Han Unique Tree -->
		<Row Tag="LOC_TREE_CIVICS_AQ_HAN_NAME">
			<Text>Han Civics</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ZHI_NAME">
			<Text>Zhi</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ZHI_QUOTE">
			<Text>"I do not open up the truth to one who is not eager to get knowledge."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ZHI_QUOTE_AUTHOR">
			<Text>Confucius</Text>
		</Row>
		<Row Tag="LOC_CIVIC_LI_NAME">
			<Text>Li</Text>
		</Row>
		<Row Tag="LOC_CIVIC_LI_QUOTE">
			<Text>"Name now our names, praise us. We are your mother, we are your father. Speak now."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_LI_QUOTE_AUTHOR">
			<Text>Popol Vuh</Text>
		</Row>
		<Row Tag="LOC_CIVIC_YI_NAME">
			<Text>Yi</Text>
		</Row>
		<Row Tag="LOC_CIVIC_YI_QUOTE">
			<Text>"Riches and honors acquired by unrighteousness are to me as a floating cloud."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_YI_QUOTE_AUTHOR">
			<Text>Confucius</Text>
		</Row>
		<Row Tag="LOC_CIVIC_JUNZI_NAME">
			<Text>Junzi</Text>
		</Row>
		<Row Tag="LOC_CIVIC_JUNZI_QUOTE">
			<Text>"Looked at from a distance, the scholar appears stern; when approached, he is mild; when he is heard to speak, his language is firm."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_JUNZI_QUOTE_AUTHOR">
			<Text>Confucius</Text>
		</Row>
		<!-- Khmer Unique Tree -->
		<Row Tag="LOC_TREE_CIVICS_AQ_KHMER_NAME">
			<Text>Khmer Civics</Text>
		</Row>
		<Row Tag="LOC_CIVIC_MOUSONG_NAME">
			<Text>Mousong</Text>
		</Row>
		<Row Tag="LOC_CIVIC_MOUSONG_QUOTE">
			<Text>"Whose eye controls this world in highest heaven, he verily knows it, or perhaps he knows not."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_MOUSONG_QUOTE_AUTHOR">
			<Text>The Rig Veda</Text>
		</Row>
		<Row Tag="LOC_CIVIC_AMNACH_NAME">
			<Text>Amnach</Text>
		</Row>
		<Row Tag="LOC_CIVIC_AMNACH_QUOTE">
			<Text>"The king's fire was like the moon, and his enemies drooped to the ground like lotuses at night."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_AMNACH_QUOTE_AUTHOR">
			<Text>Preah Khan Inscription</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CHAKRAVARTI_NAME">
			<Text>Chakravarti</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CHAKRAVARTI_QUOTE">
			<Text>"Live with the Dhamma as your island, the Dhamma as your refuge, with nothing else as your refuge."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CHAKRAVARTI_QUOTE_AUTHOR">
			<Text>The Cakkavatti Sutta</Text>
		</Row>
		<!-- Maurya Unique Tree -->
		<Row Tag="LOC_TREE_CIVICS_AQ_MAURYA_NAME">
			<Text>Mauryan Civics</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ACHARYA_NAME">
			<Text>Acharya</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ACHARYA_QUOTE">
			<Text>"Those who know true wisdom are one with Thee, O God."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ACHARYA_QUOTE_AUTHOR">
			<Text>Vishnu Purana</Text>
		</Row>
		<Row Tag="LOC_CIVIC_VYUHAM_NAME">
			<Text>Vyuham</Text>
		</Row>
		<Row Tag="LOC_CIVIC_VYUHAM_QUOTE">
			<Text>"For those who are born, death is certain, and for those who are dead, birth is certain."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_VYUHAM_QUOTE_AUTHOR">
			<Text>The Mahābhārata</Text>
		</Row>
		<Row Tag="LOC_CIVIC_AYURVEDA_NAME">
			<Text>Ayurveda</Text>
		</Row>
		<Row Tag="LOC_CIVIC_AYURVEDA_QUOTE">
			<Text>"Preserve the health of the healthy, and break the cause of disease."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_AYURVEDA_QUOTE_AUTHOR">
			<Text>Ayurveda Motto</Text>
		</Row>
		<Row Tag="LOC_CIVIC_MANTRIPARISHAD_NAME">
			<Text>Mantriparishad</Text>
		</Row>
		<Row Tag="LOC_CIVIC_MANTRIPARISHAD_QUOTE">
			<Text>"Test a servant in duty, a relative in difficulty, a friend in adversity, and a wife in misfortune."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_MANTRIPARISHAD_QUOTE_AUTHOR">
			<Text>Chanakya</Text>
		</Row>
		<!-- Maya Unique Tree -->
		<Row Tag="LOC_TREE_CIVICS_AQ_MAYA_NAME">
			<Text>Mayan Civics</Text>
		</Row>
		<Row Tag="LOC_CIVIC_RAIN_OF_CHAAC_NAME">
			<Text>Rain of Chaac</Text>
		</Row>
		<Row Tag="LOC_CIVIC_RAIN_OF_CHAAC_QUOTE">
			<Text>"All alone the sky exists. The face of the earth has not yet appeared."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_RAIN_OF_CHAAC_QUOTE_AUTHOR">
			<Text>Popol Vuh</Text>
		</Row>
		<Row Tag="LOC_CIVIC_LORDS_OF_XIBALBA_NAME">
			<Text>Lords of Xibalba</Text>
		</Row>
		<Row Tag="LOC_CIVIC_LORDS_OF_XIBALBA_QUOTE">
			<Text>"The monarchs of Xibalba requested to be slain and resuscitated. They were killed, but the brothers refrained from resuscitating."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_LORDS_OF_XIBALBA_QUOTE_AUTHOR">
			<Text>Popol Vuh</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CALENDAR_ROUND_NAME">
			<Text>Calendar Round</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CALENDAR_ROUND_QUOTE">
			<Text>"It is 12 Ahau 8 Pax. The Holy Lord of Ixtutz drives the stone into the ground."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CALENDAR_ROUND_QUOTE_AUTHOR">
			<Text>The Geneva Stela</Text>
		</Row>
		<!-- Mississippian Unique Tree -->
		<Row Tag="LOC_TREE_CIVICS_AQ_MISSISSIPPIAN_NAME">
			<Text>Mississippian Civics</Text>
		</Row>
		<Row Tag="LOC_CIVIC_EARTHWORKS_NAME">
			<Text>Earthworks</Text>
		</Row>
		<Row Tag="LOC_CIVIC_EARTHWORKS_QUOTE">
			<Text>"The king's cup was placed on a tumulus, with many arrows set about it."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_EARTHWORKS_QUOTE_AUTHOR">
			<Text>Jacques le Moyne</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CAH_NAH_HA_NAME">
			<Text>Cah-nah-ha</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CAH_NAH_HA_QUOTE">
			<Text>"The things the worshipper gives to his gods are not the food he places on the altar, or the blood in his veins. It is his thought."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CAH_NAH_HA_QUOTE_AUTHOR">
			<Text>Émile Durkheim</Text>
		</Row>
		<Row Tag="LOC_CIVIC_WAAHIH_NAME">
			<Text>Waahih</Text>
		</Row>
		<Row Tag="LOC_CIVIC_WAAHIH_QUOTE">
			<Text>"This is a good day to die!"</Text>
		</Row>
		<Row Tag="LOC_CIVIC_WAAHIH_QUOTE_AUTHOR">
			<Text>Sitting Bull</Text>
		</Row>
		<!-- Persia Unique Tree -->
		<Row Tag="LOC_TREE_CIVICS_AQ_PERSIA_NAME">
			<Text>Persian Civics</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SPADA_NAME">
			<Text>Spada</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SPADA_QUOTE">
			<Text>"Be eager in the face of danger."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SPADA_QUOTE_AUTHOR">
			<Text>Xenophon</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SATRAPIES_NAME">
			<Text>Satrapies</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SATRAPIES_QUOTE">
			<Text>"Better to live in a rugged land and rule than to cultivate rich plains and serve."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SATRAPIES_QUOTE_AUTHOR">
			<Text>Cyrus the Great</Text>
		</Row>
		<Row Tag="LOC_CIVIC_HAZARABAM_NAME">
			<Text>Hazarabam</Text>
		</Row>
		<Row Tag="LOC_CIVIC_HAZARABAM_QUOTE">
			<Text>"Kings that, to the power of the emperor obedient, march."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_HAZARABAM_QUOTE_AUTHOR">
			<Text>Aeschylus</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ROYAL_ROAD_NAME">
			<Text>Royal Road</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ROYAL_ROAD_QUOTE">
			<Text>"A Persian has delivered battle far indeed from Persia."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ROYAL_ROAD_QUOTE_AUTHOR">
			<Text>Darius I</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ACHAEMENID_EMPIRE_NAME">
			<Text>Achaemenid Empire</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ACHAEMENID_EMPIRE_QUOTE">
			<Text>"Should I pass you by and leave you lying there because of your war against us, or should I set you up again because of your virtues?"</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ACHAEMENID_EMPIRE_QUOTE_AUTHOR">
			<Text>Plutarch</Text>
		</Row>
		<!-- Rome Unique Tree -->
		<Row Tag="LOC_TREE_CIVICS_AQ_ROME_NAME">
			<Text>Roman Civics</Text>
		</Row>
		<Row Tag="LOC_CIVIC_EXERCITUS_ROMANUS_NAME">
			<Text>Exercitus Romanus</Text>
		</Row>
		<Row Tag="LOC_CIVIC_EXERCITUS_ROMANUS_QUOTE">
			<Text>"It is easier to find men who will volunteer to die, than to find those who are willing to endure pain with patience."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_EXERCITUS_ROMANUS_QUOTE_AUTHOR">
			<Text>Julius Caesar</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CIVIS_ROMANUS_NAME">
			<Text>Civis Romanus</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CIVIS_ROMANUS_QUOTE">
			<Text>"Avoid any specific discussion of public policy at public meetings."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_CIVIS_ROMANUS_QUOTE_AUTHOR">
			<Text>Cicero</Text>
		</Row>
		<Row Tag="LOC_CIVIC_LEGATUS_PRO_PRAETORE_NAME">
			<Text>Legatus Pro Praetore</Text>
		</Row>
