		<Argument name="Percent">20</Argument>
	</Modifier>
	<Modifier id="MOD_GOLDEN_AGE_THEOCRACY_CIVILIAN" collection="COLLECTION_PLAYER_CITIES" effect="EFFECT_CITY_ADJUST_UNIT_PRODUCTION">
		<Argument name="UnitCoreClass">CORE_CLASS_CIVILIAN</Argument>
		<Argument name="Percent">40</Argument>
	</Modifier>
	<Modifier id="MOD_GOLDEN_AGE_THEOCRACY_SUPPORT" collection="COLLECTION_PLAYER_CITIES" effect="EFFECT_CITY_ADJUST_UNIT_PRODUCTION">
		<Argument name="UnitCoreClass">CORE_CLASS_SUPPORT</Argument>
		<Argument name="Percent">40</Argument>
	</Modifier>
	<Modifier id="MOD_GOLDEN_AGE_THEOCRACY_RECON" collection="COLLECTION_PLAYER_CITIES" effect="EFFECT_CITY_ADJUST_UNIT_PRODUCTION">
		<Argument name="UnitCoreClass">CORE_CLASS_RECON</Argument>
		<Argument name="Percent">40</Argument>
	</Modifier>
