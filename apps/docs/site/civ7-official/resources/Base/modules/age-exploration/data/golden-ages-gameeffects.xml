		<Argument name="Percent">20</Argument>
	</Modifier>
	<Modifier id="MOD_GOLDEN_AGE_PLUTOCRACY_OVERBUILDING" collection="COLLECTION_PLAYER_CITIES" effect="EFFECT_CITY_ADJUST_CONSTRUCTIBLE_PRODUCTION">
