	<Modifier id="MOD_GOLDEN_AGE_REVOLUTIONARY_AUTHORITARIANISM_UNIT_PRODUCTION" collection="COLLECTION_PLAYER_CITIES" effect="EFFECT_CITY_ADJUST_UNIT_PRODUCTION">
		<Argument name="YieldType">YIELD_PRODUCTION</Argument>
		<Argument name="Percent">100</Argument>
		<Argument name="UnitCoreClass">CORE_CLASS_MILITARY</Argument>
	</Modifier>
	<Modifier id="MOD_GOLDEN_AGE_REVOLUTIONARY_AUTHORITARIANISM_ENVOYS" collection="COLLECTION_OWNER" effect="EFFECT_DIPLOMACY_ADJUST_DIPLOMATIC_ACTION_TOKEN_BONUS">
		<Argument name="ActionType">DIPLOMACY_ACTION_DECLARE_WAR</Argument>
		<Argument name="OnlyWhenInitiator">true</Argument>
		<Argument name="Amount">6</Argument>
	</Modifier>
	<!-- Constitutional Monarchy -->
	<Modifier id="MOD_GOLDEN_AGE_CONSTITUTIONAL_MONARCHY_GOLD_PERCENT" collection="COLLECTION_OWNER" effect="EFFECT_PLAYER_ADJUST_YIELD">
