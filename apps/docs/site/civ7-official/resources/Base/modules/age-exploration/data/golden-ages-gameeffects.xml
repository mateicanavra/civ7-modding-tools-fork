<?xml version="1.0" encoding="utf-8"?>
<GameEffects xmlns="GameEffects">
	<!-- Feudal Monarchy -->
	<Modifier id="MOD_GOLDEN_AGE_FEUDAL_MONARCHY_FOOD" collection="COLLECTION_PLAYER_CITIES" effect="EFFECT_CITY_ADJUST_YIELD">
		<Argument name="YieldType">YIELD_FOOD</Argument>
		<Argument name="Percent">20</Argument>
	</Modifier>
	<Modifier id="MOD_GOLDEN_AGE_FEUDAL_MONARCHY_CAVALRY" collection="COLLECTION_PLAYER_CITIES" effect="EFFECT_CITY_ADJUST_UNIT_PRODUCTION">
		<Argument name="UnitClass">UNIT_CLASS_CAVALRY</Argument>
		<Argument name="Percent">30</Argument>
	</Modifier>
	<Modifier id="MOD_GOLDEN_AGE_FEUDAL_MONARCHY_NAVAL" collection="COLLECTION_PLAYER_CITIES" effect="EFFECT_CITY_ADJUST_UNIT_PRODUCTION">
		<Argument name="UnitClass">UNIT_CLASS_NAVAL</Argument>
		<Argument name="Percent">30</Argument>
	</Modifier>
