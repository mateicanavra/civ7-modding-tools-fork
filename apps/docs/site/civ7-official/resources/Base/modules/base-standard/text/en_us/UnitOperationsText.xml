<?xml version="1.0" encoding="utf-8"?>
<Database>
	<EnglishText>
		<Row Tag="LOC_UNITOPERATION_AERIAL_RECON_NAME">
			<Text>Aerial Reconnaissance</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_AERIAL_RECON_DESCRIPTION">
			<Text>Grants sight on a previously revealed tile within 8 tiles of the commander. All adjacent tiles are also revealed. Reveals reinforcement paths and Stealth Units in the area.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_ALERT_NAME">
			<Text>Alert</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_ALERT_DESCRIPTION">
			<Text>Wake Unit when an enemy is nearby.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_AUTOMATE_EXPLORE_NAME">
			<Text>Automate Exploration</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_AUTOMATE_EXPLORE_DESCRIPTION">
			<Text>Automate exploration of unrevealed territory.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_CARGO_DROP_NAME">
			<Text>Cargo Drop</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_CARGO_DROP_DESCRIPTION">
			<Text>Heal a friendly Unit, along with all adjacent friendly Units.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_UNIT_AIR_DROP_NAME">
			<Text>Airdrop</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_UNIT_AIR_DROP_DESCRIPTION">
			<Text>Drop all packed Infantry Units at a selected location.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_CREATE_RELIC_NAME">
			<Text>Create Relic</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_CREATE_RELIC_DESCRIPTION">
			<Text>Create Relic.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_DISPERSE_INDEPENDENT_NAME">
			<Text>Disperse Independent</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_DISPERSE_INDEPENDENT_DESCRIPTION">
			<Text>Disperse Independent.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_EXCAVATE_NAME">
			<Text>Excavate Artifact</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_EXCAVATE_DESCRIPTION">
			<Text>Dig up a Great Work that can be displayed in a Museum to make progress on the Geographic Society Legacy Path.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_FOUND_CITY_NAME">
			<Text>Found Settlement</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_FOUND_CITY_DESCRIPTION">
			<Text>Found Settlement. It cannot be within 3 tiles of another Settlement or Independent Power.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_GIVE_INFLUENCE_TOKEN_NAME">
			<Text>Befriend Independent</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_GIVE_INFLUENCE_TOKEN_DESCRIPTION">
			<Text>Befriend Independent.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_HEAL_NAME">
			<Text>Heal</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_HEAL_DESCRIPTION">
			<Text>Rest until fully healed.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_EXECUTE_SCRIPT_NAME">
			<Text>Execute Script</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_EXECUTE_SCRIPT_DESCRIPTION">
			<Text>Execute Script.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_MOVE_TO_NAME">
			<Text>Move</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_MOVE_TO_DESCRIPTION">
			<Text>Move to a specified location.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_MOVE_TO_UNIT_NAME">
			<Text>Move to Unit</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_MOVE_TO_UNIT_DESCRIPTION">
			<Text>Move to Unit.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_PILLAGE_NAME">
			<Text>Pillage</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_PILLAGE_DESCRIPTION">
			<Text>Pillage this location.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_RANGE_ATTACK_NAME">
			<Text>Range Attack</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_RANGE_ATTACK_DESCRIPTION">
			<Text>Range Attack.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_RECEIVE_GIFT_NAME">
			<Text>Send Envoy</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_RECEIVE_GIFT_DESCRIPTION">
			<Text>Send an Envoy.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_RESEARCH_ARTIFACTS_NAME">
			<Text>Research Artifacts</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_RESEARCH_ARTIFACTS_DESCRIPTION">
			<Text>Find Ruin sites containing Artifacts on this Continent.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_ROUTE_TO_NAME">
			<Text>Route to</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_ROUTE_TO_DESCRIPTION">
			<Text>Establish a route to a specified location.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_SKIP_TURN_NAME">
			<Text>Skip Turn</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_SKIP_TURN_DESCRIPTION">
			<Text>Skip current turn action.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_SLEEP_NAME">
			<Text>Sleep</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_SLEEP_DESCRIPTION">
			<Text>Order this Unit to sleep until awakened.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_SPREAD_RELIGION_NAME">
			<Text>Spread Religion</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_SPREAD_RELIGION_DESCRIPTION">
			<Text>Convert this Settlement's population to your Religion.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_TELEPORT_TO_CITY_NAME">
			<Text>Travel to City</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_TELEPORT_TO_CITY_DESCRIPTION">
			<Text>Moves the unit from one Capital City to another.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_TELEPORT_TO_DESCRIPTION">
			<Text>Travel to</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_UPGRADE_NAME">
			<Text>Upgrade</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_UPGRADE_DESCRIPTION">
			<Text>Upgrade to {1_UnitTypeName}.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_WAIT_FOR_NAME">
			<Text>Wait For</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_WAIT_FOR_DESCRIPTION">
			<Text>Wait For</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_WMD_STRIKE_NAME">
			<Text>WMD Strike</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_WMD_STRIKE_DESCRIPTION">
			<Text>Detonate a weapon of mass destruction on a location.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_ESTABLISH_BASE_NAME">
			<Text>Establish Air Base</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_ESTABLISH_BASE_DESCRIPTION">
			<Text>Establish Air Base at a new location.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_REBASE_NAME">
			<Text>Rebase Aircraft</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_REBASE_DESCRIPTION">
			<Text>Move Aircraft to a new Air Base.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_AIR_ATTACK_NAME">
			<Text>Air Strike</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_AIR_ATTACK_DESCRIPTION">
			<Text>Initiate an air strike on a specific location.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_NAVAL_ATTACK_NAME">
			<Text>Naval Attack</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_NAVAL_ATTACK_DESCRIPTION">
			<Text>Initiate a naval strike on a specific location.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_RELIGIOUS_HEAL_NAME">
			<Text>Heal</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_RELIGIOUS_HEAL_DESCRIPTION">
			<Text>Heal Units within 1 tile 40 HP.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_FORTIFY_NAME">
			<Text>Fortify</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_FORTIFY_DESCRIPTION">
			<Text>Build temporary fortifications.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_REINFORCE_ARMY_NAME">
			<Text>Reinforce Army</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_REINFORCE_ARMY_DESCRIPTION">
			<Text>Reinforce the Army of a selected Commander. Unit is removed from map during travel time.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_CALL_REINFORCEMENTS_NAME">
			<Text>Call Reinforcements</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_CALL_REINFORCEMENTS_DESCRIPTION">
			<Text>Request an eligible Unit to reinforce your Army. Unit is removed from map during travel time.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_COASTAL_RAID_NAME">
			<Text>Coastal Raid</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_COASTAL_RAID_DESCRIPTION">
			<Text>Pillage and Capture an adjacent tile if able. Gain Gold and repair damage. May also trigger adjacent discoveries.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_LOOKOUT_NAME">
			<Text>Lookout</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_LOOKOUT_DESCRIPTION">
			<Text>Increase sight range +1.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_SEARCH_NAME">
			<Text>Search</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_SEARCH_DESCRIPTION">
			<Text>Increase sight range +1 for 1 turn. Reveal all Discoveries within twice this range.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_EMBED_ADJUST_ABILITY_SELF_DESCRIPTION">
			<Text>Embed: activate an Ability on this Unit.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_EMBED_ADJUST_ABILITY_SELF_REQ_WATER_TERRAIN_DESCRIPTION">
			<Text>Embed: activate an Ability on this Unit. Must be located on water Terrain (Coast or Ocean).</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_EMBED_ADJUST_ABILITY_SELF_REQ_FOREIGN_SETTLEMENT_DESCRIPTION">
			<Text>Embed: activate an Ability on this Unit. Must be located in a foreign Settlement.</Text>
		</Row>
		<!--Operation Results Text-->
		<Row Tag="LOC_UNITOPERATION_CANNOT_USE_WHILE_EMBARKED">
			<Text>Cannot use while Embarked.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_REQUIRES_MOVEMENT">
			<Text>Requires Movement.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_REQUIRES_PROMOTION">
			<Text>Requires the {1_promo} promotion.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_REQUIRES_ELIGIBLE_UNIT">
			<Text>No eligible Units in range.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_ESTABLISH_BASE_FAILED">
			<Text>No eligible Air Base locations.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_ESTABLISH_BASE_REQUIRED">
			<Text>Requires establishing an Air Base.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_TARGET_REQUIRED">
			<Text>No valid targets.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_NO_AIR_BASES_WITH_SLOTS">
			<Text>No eligible air bases with enough slots for all packed air units in range.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_UNPACK_LAND_UNITS">
			<Text>Must unpack land units before rebasing.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_NO_PACKED_AIR_UNITS">
			<Text>Requires an eligible air unit based with this commander.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_CANNOT_HEAL_OUTSIDE_FRIENDLY_AREA">
			<Text>Cannot heal outside friendly territory unless adjacent to a friendly Logistics Improvement.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_CANNOT_HEAL_WHILE_EMBARKED">
			<Text>Land Units cannot heal while Embarked.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_CANNOT_RESETTLE_OUTSIDE_OWNED_URBAN_PLOT">
			<Text>Must be located on an owned District.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_CANNOT_BE_DAMAGED">
			<Text>Unit cannot be damaged.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_REQUIRE_OWNED_PLOT">
			<Text>Can only be used inside owned territory.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_REQUIRE_FOREIGN_SETTLEMENT_PLOT">
			<Text>Can only be used in another Civilization's owned territory.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_PILLAGE_HEAL_DESCRIPTION">
			<Text>Damage the {1_ConstructibleName} and heal {2_HealingAmount} HP.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_PILLAGE_GOLD_DESCRIPTION">
			<Text>Damage the {1_ConstructibleName} and gain {2_GoldAmount} Gold.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_PILLAGE_SCIENCE_DESCRIPTION">
			<Text>Damage the {1_ConstructibleName} and gain {2_ScienceAmount} Science.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_PILLAGE_CULTURE_DESCRIPTION">
			<Text>Damage the {1_ConstructibleName} and gain {2_CultureAmount} Culture.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_PILLAGE_DIPLOMACY_DESCRIPTION">
			<Text>Damage the {1_ConstructibleName} and gain {2_DiplomacyAmount} Influence.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_PILLAGE_FOOD_TOWN_DESCRIPTION">
			<Text>Damage the {1_ConstructibleName} and gain {2_FoodAmount} Food to all Towns.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_PILLAGE_PRODUCTION_CITY_DESCRIPTION">
			<Text>Damage the {1_ConstructibleName} and gain {2_ProductionAmount} Production to all Cities.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_REQUIRE_WATER_TERRAIN">
			<Text>Must be located on water Terrain (Coast or Ocean).</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_NO_AVAILABLE_RELIGION_LOCATION">
			<Text>Must be located on a valid District to Spread Religion.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_CANNOT_CONVERT_RAZING_CITY">
			<Text>Spread Religion cannot be used on Settlements that are being razed.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_NO_AVAILABLE_RELIGION_ALREADY_FOLLOWING">
			<Text>This location already follows this Missonary's Religion.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_RELIGION_LOCATION">
			<Text>Must be located in a Settlement following your Religion.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_RELIC_LOCATION">
			<Text>This location must have a Relic slot available.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_NO_MOVEMENT_LEFT">
			<Text>Unit has no movement left.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_NOT_AT_RESEARCH_SITE">
			<Text>Ruins have not been researched for this continent. Move the Explorer to a University or Museum within the continent to find the dig sites.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_CONTINENT_ALREADY_RESEARCHED">
			<Text>Ruins already researched for this continent, but still unknown for {1_Continents: plural 1?{1_Continents} continent;other?{1_Continents} continents;}. Move the Explorer to a University or Museum on a different continent to find more dig sites.</Text>
		</Row>



		<Row Tag="LOC_UNITOPERATION_RESEARCH_ARTIFACTS_1_AVAILABLE">
			<Text>This continent's Exploration Age Ruins can be researched by an Explorer at a University or Museum.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_RESEARCH_ARTIFACTS_1_RESEARCHED">
			<Text>Exploration Ruins have already been researched for this continent. Complete the Hegemony Civic to unlock research of Antiquity Ruins.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_RESEARCH_ARTIFACTS_2_AVAILABLE">
			<Text>This continent's Ruins (from all Ages) can be researched by an Explorer at a University or Museum.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_RESEARCH_ARTIFACTS_1_AVAIL_1_RESEARCH">
			<Text>This continent's Antiquity Age Ruins can be researched by an Explorer at a University or Museum.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_RESEARCH_ARTIFACTS_2_RESEARCHED">
			<Text>Ruins from all Ages have already been researched for this continent.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_WMDSTRIKE_NO_AVAILABLE_WARHEAD">
			<Text>No Available Warhead.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_WMDSTRIKE_AVAILABLE_WARHEADS">
			<Text>Available Nuclear Weapons: {1_Count}.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_WMDSTRIKE_NO_CAPABLE_AIRCRAFT">
			<Text>No packed units are currently capable of deploying a WMD.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_REINFORCE_ARMY_NO_ELIGIBLE_COMMANDER">
			<Text>There are no eligible Commanders to reinforce. Commander must be at least {1_MinimumDistance} tiles away.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_REINFORCE_ARMY_NOT_WHEN_EMBARKED">
			<Text>Embarked Armies cannot be reinforced.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_NO_VALID_CLAIM_LOCATIONS">
			<Text>There are no valid locations to Settle.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_NO_VALID_PILLAGE_LOCATIONS">
			<Text>There are no valid locations to pillage.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_CAPTURE_DISTRICT_MUST_NOT_CONTAIN_UNITS">
			<Text>There are no valid capturable locations.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_NO_REINFORCEMENTS_AVAILABLE">
			<Text>No Unit reinforcements available.</Text>
		</Row>
		<!--Operation Dialog Text-->
		<Row Tag="LOC_UNITOPERATION_PILLAGE_CONFIRM_FRIENDLY_TITLE">
			<Text>Pillage Friendly Tile?</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_PILLAGE_CONFIRM_FRIENDLY_BODY">
			<Text>This tile is friendly territory. Are you sure you wish to proceed?</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_PILLAGE_CONFIRM_FRIENDLY_CITY_BODY">
			<Text>This tile is friendly territory. Damaging it will cause significant Unhappiness to the Settlement of {1_Name}. Are you sure you want to proceed?</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_EXTRACT_ARTIFACT_IN_PROGRESS">
			<Text>Excavation will be complete in {1_Turns} {1_Turns: plural 1?turn; other?turns;}.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_EXTRACT_ARTIFACT_IN_PROGRESS_OTHER_UNIT">
			<Text>An Excavation has already begun here.</Text>
		</Row>
		<Row Tag="LOC_UNITOPERATION_MOVE_RAIL_NOT_ADJACENT_RAILYARD">
			<Text>Must be in or adjacent to a Rail Station.</Text>
		</Row>
	</EnglishText>
</Database>
