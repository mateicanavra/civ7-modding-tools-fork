		<property name="AgeReference" path="AgeRef"/>
	</definition>
	<definition name="AgeGrowthBalance" table-name="AgeGrowthBalances" cpp="true">
		<field name="Age" type="Text" key="true" not-null="true"/>
		<field name="Flat" type="Integer" not-null="true"/>
		<field name="Scalar" type="Integer" not-null="true"/>
		<field name="Exponent" type="Integer" not-null="true"/>
