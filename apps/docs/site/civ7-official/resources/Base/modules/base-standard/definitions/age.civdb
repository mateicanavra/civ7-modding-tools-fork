	<definition name="AgeConstructibleDefaultMaintenance" table-name="AgeConstructibleDefaultMaintenances" cpp="true">
		<field name="Age" type="Text" key="true" not-null="true"/>
		<field name="YieldChange" type="Integer" not-null="true"/>
