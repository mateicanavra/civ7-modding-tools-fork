		<reference source="MapSizeType" target="MapSizeType" sourceRoleName="MapSizeRef" targetRoleName="AgeProgressionEventRef" to="Map"/>
	</definition>
	<definition name="AgeProgressionNotification" table-name="AgeProgressionNotifications" cpp="true">
		<field name="NotificationType" type="Text" not-null="true" />
