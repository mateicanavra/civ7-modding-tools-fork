		<reference source="MainTechProgressionTreeType" target="ProgressionTreeType" sourceRoleName="MainTechProgressionTreeRef" on-delete="SetNULL" to="ProgressionTree"/>
		<reference source="TradeSystemParameterSet" target="Type" sourceRoleName="TradeSystemParameterSetRef" on-delete="SetNULL" to="TradeSystemParameterSet"/>
		<property name="MainCultureProgressionTreeReference" path="MainCultureProgressionTreeRef"/>
		<property name="MainTechProgressionTreeReference" path="MainTechProgressionTreeRef"/>
