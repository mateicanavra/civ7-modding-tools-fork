		<reference source="ValidAgeType" target="AgeType" to="Age" />
	</definition>
	<definition name="AgeProgressionEvent" table-name="AgeProgressionEvents" cpp="true">
		<field name="AgeProgressionEventType" type="Text" key="true" not-null="true" hash="true"/>
		<field name="AgeType" type="Text" not-null="true" hash="true"/>
