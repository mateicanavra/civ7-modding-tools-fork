		<property name="AgeReference" path="AgeRef"/>
	</definition>
	<definition name="AgeProgressionReward" table-name="AgeProgressionRewards" cpp="true">
		<!-- The milestone reward type-->
		<field name="AgeProgressionRewardType" type="Text" key="true" not-null="true" hash="true"/>
		<!-- The modifier the reward will apply -->
		<field name="ModifierId" type="Text" not-null="true"/>
		<!-- The reward details -->
		<field name="Name" type="LocalizedText"/>
		<!-- The description of the reward -->
		<field name="Description" type="LocalizedText"/>
		<!-- The description of the reward, to be used if it is the final age, and different from non-final ages -->
		<field name="DescriptionFinalAge" type="LocalizedText"/>
		<!-- The icon show on the UI for the reward -->
		<field name="Icon" type="Text"/>
	</definition>
	<definition name="AgeProgressionMilestone" table-name="AgeProgressionMilestones" cpp="true">
		<!-- The type of milestone, e.g ANTIQUITY_SCIENCE_MILESTONE_1 -->
		<field name="AgeProgressionMilestoneType" type="Text" key="true" not-null="true" hash="true"/>
		<!-- The legacy path this milestone belongs to science, military, economic or culture -->
		<field name="LegacyPathType" type="Text" not-null="true" hash="true"/>
		<!-- The number of path points to reach the milestone e.g 4 codicies or 2 wonders -->
		<field name="RequiredPathPoints" type="Integer" value="0" not-null="true"/>
		<!-- The amount of age progression added to the age when reached -->
		<field name="AgeProgressionAmount" type="Integer" value="0" not-null="true"/>
		<!-- Will this milestone trigger a victory unlocking, or unlock a golden age card? -->
		<field name="FinalMilestone" type="Bool" value="0" not-null="true"/>

		<reference source="AgeProgressionMilestoneType" target="Type" to="Type"/>
		<reference source="LegacyPathType" target="LegacyPathType" to="LegacyPath" />
	</definition>
	<definition name="AgeProgressionMilestoneReward" table-name="AgeProgressionMilestoneRewards" cpp="true">
		<!-- The type of milestone, e.g ANTIQUITY_SCIENCE_MILESTONE_1 -->
		<field name="AgeProgressionMilestoneType" type="Text" key="true" not-null="true" hash="true"/>
		<!-- The milestone reward type-->
		<field name="AgeProgressionRewardType" type="Text" key="true" not-null="true" hash="true"/>
		<!-- Whether or not to apply immediately-->
		<field name="IsImmediate" type="Bool" value="0" not-null="true"/>

		<reference source="AgeProgressionMilestoneType" target="AgeProgressionMilestoneType" to="AgeProgressionMilestone" />
		<reference source="AgeProgressionRewardType" target="AgeProgressionRewardType" to="AgeProgressionReward" />
	</definition>
	<definition name="AgeProgressionDarkAgeRewardInfo" table-name="AgeProgressionDarkAgeRewardInfos" cpp="true">
		<!-- The legacy path this milestone belongs to science, military, economic or culture -->
		<field name="LegacyPathType" type="Text" not-null="true" hash="true"/>
		<!-- The reward details -->
		<field name="Name" type="LocalizedText"/>
		<!-- The description of the reward -->
		<field name="Description" type="LocalizedText"/>
		<!-- The icon show on the UI for the reward -->
		<field name="Icon" type="Text"/>
		<reference source="LegacyPathType" target="LegacyPathType" to="LegacyPath" />
	</definition>
	<definition name="AgeCrisisStage" table-name="AgeCrisisStages" cpp="true">
		<field name="Stage" type="Integer" key="true" not-null="true"/>
		<field name="AgeCrisisEventType" type="Text" key="true" not-null="true" hash="true"/>
		<field name="MinDuration" type="Integer" value="0" not-null="true"/>
		<field name="AgeProgressTriggerPercent" type="Integer" value="0" not-null="true"/>
		<field name="AgeProgressEndPercent" type="Integer" value="0" not-null="true"/>
		<field name="FinalStage" type="Bool" value="0" not-null="true"/>
	</definition>
	<definition name="AgeCrisisNarrative" table-name="AgeCrisisNarratives" cpp="true">
		<field name="NarrativeStoryType" type="Text" key="true" not-null="true" hash="true"/>
