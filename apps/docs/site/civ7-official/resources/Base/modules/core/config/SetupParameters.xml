	<Queries>
		<!-- Parameter Queries -->
		<Row QueryId="Parameters" Type="Parameters" SQL="SELECT * FROM Parameters P WHERE Key1 IS NULL and Key2 IS NULL"/>
		<Row QueryId="AgeParameters" Type="Parameters" SQL="SELECT * FROM Parameters P WHERE Key1='Age' AND Key2= ?1"/>
		<Row QueryId="RulesetParameters" Type="Parameters" SQL="SELECT * FROM Parameters P WHERE Key1='Ruleset' AND Key2 = ?1"/>
		<Row QueryId="MapParameters" Type="Parameters" SQL="SELECT * FROM Parameters P WHERE Key1='Map' AND Key2 = ?1"/>
		<Row QueryId="DefeatParameters" Type="Parameters" SQL="SELECT DefeatType AS ParameterID, D.Name AS Name, D.Description AS Description, Visible, ReadOnly, 1 AS SupportsSinglePlayer, 1 AS SupportsLANMultiplayer, 1 AS SupportsInternetMultiplayer, 0 AS ChangeableAfterGameStart, 'bool' AS Domain, 0 AS Hash, 1 AS DefaultValue, 'Game' AS ConfigurationGroup, DefeatType AS ConfigurationKey, 'Victories' AS GroupId, 10 AS SortIndex FROM Defeats D, Ages WHERE AgeType = ?1 and D.Domain = Ages.DefeatDomain"/>
		<Row QueryId="VictoryParameters" Type="Parameters" SQL="SELECT VictoryType AS ParameterID, V.Name AS Name, V.Description AS Description, Visible, ReadOnly, 1 AS SupportsSinglePlayer, 1 AS SupportsLANMultiplayer, 1 AS SupportsInternetMultiplayer, 0 AS ChangeableAfterGameStart, 'bool' AS Domain, 0 AS Hash, 1 AS DefaultValue, 'Game' AS ConfigurationGroup, VictoryType AS ConfigurationKey, 'Victories' AS GroupId, 20 AS SortIndex FROM Victories V, Ages WHERE AgeType =?1 and V.Domain = Ages.VictoryDomain"/>
		<!-- Parameter Criteria Queries -->
		<Row QueryId="ParameterCriteria" Type="ParameterCriteria" SQL="SELECT * FROM ParameterCriteria ORDER BY ParameterID"/>
		<Row QueryId="FixedMaxTurnsCriteria" Type="ParameterCriteria" SQL="SELECT 'MaxTurns' AS ParameterID, 'Game' AS ConfigurationGroup, 'AGE' AS ConfigurationKey, 'NotEquals' AS Operator, AgeType AS ConfigurationValue FROM Ages WHERE FixedMaxTurns = 1"/>
		<!-- Parameter Dependency Queries -->
		<Row QueryId="ParameterDependencies" Type="ParameterDependencies" SQL="SELECT * FROM ParameterDependencies ORDER BY ParameterID"/>
		<!-- Disable 'Resources' drop-down if map is fixed map, e.g pre-populated with resources. -->
		<Row QueryId="MapResourcesDependencies" Type="ParameterDependencies" SQL="SELECT 'Resources' AS ParameterID, 'Map' AS ConfigurationGroup, 'MAP_SCRIPT' AS ConfigurationKey, 'NotEquals' AS Operator, File AS ConfigurationValue FROM Maps WHERE StaticMap = 1"/>
		<!-- Value Queries -->
		<Row QueryId="Ages" Type="DomainValues" SQL="SELECT Domain, Name, Description, AgeType AS Value, Banner, SortIndex FROM Ages"/>
		<Row QueryId="DomainValues" Type="DomainValues" SQL="SELECT * FROM DomainValues WHERE Key1 IS NULL AND Key2 IS NULL"/>
		<Row QueryId="Civilizations" Type="DomainValues" SQL="SELECT Domain, CivilizationType AS Value, CivilizationName as Name, CivilizationDescription AS Description, CivilizationIcon as Icon, SortIndex FROM Civilizations"/>
		<Row QueryId="Difficulties" Type="DomainValues" SQL="SELECT Domain, Name, Description, DifficultyType AS Value, SortIndex FROM Difficulties"/>
		<Row QueryId="GameSpeeds" Type="DomainValues" SQL="SELECT Domain, Name, Description, GameSpeedType AS Value, SortIndex FROM GameSpeeds"/>
		<Row QueryId="Leaders" Type="DomainValues" SQL="SELECT Domain, LeaderType AS Value, LeaderName as Name, LeaderDescription AS Description, LeaderIcon as Icon, SortIndex FROM Leaders"/>
		<Row QueryId="Maps" Type="DomainValues" SQL="SELECT Domain, Name, Description, File AS Value, SortIndex FROM Maps WHERE (not WorldBuilderOnly or ?)"/>
		<Row QueryId="MapSizes" Type="DomainValues" SQL="SELECT Domain, Name, Description, MapSizeType AS Value, MinPlayers, MaxPlayers, MaxHumans, DefaultPlayers, SortIndex FROM MapSizes"/>
		<Row QueryId="Mementos" Type="DomainValues" SQL="SELECT Mementos.Domain, Mementos.Name, Mementos.Description, Mementos.FunctionalDescription, Mementos.Type AS Value, Mementos.SortIndex, Rewards.CustomData AS Icon FROM Mementos INNER JOIN Rewards ON Mementos.Type = Rewards.GameItemID"/>

		<Row QueryId="LegacyPaths" Type="DomainValues" SQL="SELECT Domain, Name, Description, LegacyPathClassName, AgeType, Category, LegacyPathClassType, LegacyPathClassName, LegacyPathType AS Value, SortIndex FROM LegacyPaths"/>
		<Row QueryId="Crises" Type="DomainValues" SQL="SELECT Domain, Name, Description, Category, AgeCrisisEventType AS Value, AgeType, SortIndex FROM Crises"/>

		<!-- TODO - Bring this back once we fix mode specification. -->
