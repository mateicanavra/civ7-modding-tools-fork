		<Row QueryId="Rulesets" Type="DomainValues" SQL="SELECT 'Rulesets' AS Domain, Name, Description, RulesetType AS Value, SortIndex FROM Rulesets"/>
		<Row QueryId="TurnTimers" Type="DomainValues" SQL="SELECT Domain, Name, Description, TurnTimerType AS Value, SortIndex FROM TurnTimers"/>
		<Row QueryId="TurnPhases" Type="DomainValues" SQL="SELECT Domain, Name, Description, TurnPhaseType AS Value, SortIndex FROM TurnPhases"/>
		<!-- Value Filter Queries -->
		<Row QueryId="DomainValueFilters" Type="DomainValueFilters" SQL="SELECT * FROM DomainValueFilters WHERE Key1 IS NULL AND Key2 IS NULL"/>
		<!-- Value Union Queries -->
		<Row QueryId="DomainValueUnions" Type="DomainValueUnions" SQL="SELECT * FROM DomainValueUnions WHERE Key1 IS NULL AND Key2 IS NULL"/>
		<!-- Range Queries -->
		<Row QueryId="DomainRanges" Type="DomainRanges" SQL="SELECT * FROM DomainRanges"/>
		<!-- Map Specific Queries -->
		<!-- These queries adjust the domain values based on the selected map. -->
		<Row QueryId="MapLeaders" Type="DomainValueFilters" SQL="SELECT 'Players:*' AS Domain, LeaderType as Value, 'Intersect' as Filter FROM MapLeaders WHERE Map = ?1"/>
		<Row QueryId="DomainOverridesByMap" Type="ParameterDomainOverrides" SQL="SELECT ParameterID, Domain FROM DomainOverridesByMap WHERE Map = ?1 and (PlayerId = ?2 or PlayerId is null)"/>
		<Row QueryId="SupportedValuesByMap" Type="DomainValueFilters" SQL="SELECT Domain, Value, 'Intersect' AS Filter FROM SupportedValuesByMap WHERE Map = ?1 and (PlayerId = ?2 or PlayerId is null)"/>
		<Row QueryId="UnSupportedValuesByMap" Type="DomainValueFilters" SQL="SELECT Domain, Value, 'Difference' AS Filter FROM UnSupportedValuesByMap WHERE Map = ?1 and (PlayerId = ?2 or PlayerId is null)"/>
		<!-- Ruleset Specific queries -->
		<!-- These queries adjust the domain values based on the selected ruleset. -->
		<Row QueryId="DomainOverridesByRuleset" Type="ParameterDomainOverrides" SQL="SELECT ParameterID, Domain FROM DomainOverridesByRuleset WHERE RulesetType = ?1 and (PlayerId = ?2 or PlayerId is null)"/>
		<Row QueryId="SupportedValuesByRuleset" Type="DomainValueFilters" SQL="SELECT Domain, Value, 'Intersect' AS Filter FROM SupportedValuesByRuleset WHERE RulesetType = ?1 and (PlayerId = ?2 or PlayerId is null)"/>
		<Row QueryId="UnSupportedValuesByRuleset" Type="DomainValueFilters" SQL="SELECT Domain, Value, 'Difference' AS Filter FROM UnSupportedValuesByRuleset WHERE RulesetType = ?1 and (PlayerId = ?2 or PlayerId is null)"/>
		<!-- Age Specific queries -->
		<!-- These queries adjust the domain values based on the selected ruleset. -->
		<Row QueryId="DomainOverridesByAge" Type="ParameterDomainOverrides" SQL="SELECT ParameterID, Domain FROM DomainOverridesByAge WHERE Make_Hash(AgeType) = ?1 and (PlayerId = ?2 or PlayerId is null)"/>
		<Row QueryId="PlayerCivilizationsByAge" Type="ParameterDomainOverrides" SQL="SELECT 'PlayerCivilization' as ParameterID, PlayerCivilizationDomain as Domain FROM Ages WHERE Make_Hash(AgeType) = ?1 LIMIT 1"/>
		<Row QueryId="MapSizesByAge" Type="ParameterDomainOverrides" SQL="SELECT 'MapSize' as ParameterID, MapSizeDomain as Domain FROM Ages WHERE Make_Hash(AgeType) = ?1 LIMIT 1"/>
		<Row QueryId="SupportedValuesByAge" Type="DomainValueFilters" SQL="SELECT Domain, Value, 'Intersect' AS Filter FROM SupportedValuesByAge WHERE Make_Hash(AgeType) = ?1 and (PlayerId = ?2 or PlayerId is null)"/>
		<Row QueryId="UnSupportedValuesByAge" Type="DomainValueFilters" SQL="SELECT Domain, Value, 'Difference' AS Filter FROM UnSupportedValuesByAge WHERE Make_Hash(AgeType) = ?1 and (PlayerId = ?2 or PlayerId is null)"/>
		<!-- If the Age has 'FixedMaxTurns' then restrict the turn limit to custom. -->
		<Row QueryId="FixedMaxTurns" Type="DomainValueFilters" SQL="SELECT 'StandardTurnLimits' as Domain, 'TURNLIMIT_CUSTOM' as Value, 'Intersect' AS Filter FROM Ages WHERE Make_Hash(AgeType) = ?1 AND FixedMaxTurns = 1"/>
		<!-- Configuration Update queries -->
		<Row QueryId="ConfigurationUpdates" Type="ConfigurationUpdates" SQL="SELECT * FROM ConfigurationUpdates WHERE Key1 IS NULL AND Key2 IS NULL"/>
		<Row QueryId="ConfigurationUpdatesByAge" Type="ConfigurationUpdates" SQL="SELECT * FROM ConfigurationUpdates WHERE Key1='Age' AND Key2 = ?1"/>
		<Row QueryId="ConfigurationUpdatesByRuleset" Type="ConfigurationUpdates" SQL="SELECT * FROM ConfigurationUpdates WHERE Key1='Ruleset' AND Key2 = ?1"/>
		<Row QueryId="TurnLimitNoneUpdates" Type="ConfigurationUpdates" SQL="SELECT 'Game' AS SourceGroup, 'StartAge' AS SourceKey, AgeType AS SourceValue, 'Game' AS TargetGroup, 'TurnLimitType' AS TargetKey, 'TURNLIMIT_NONE' AS TargetValue, 1 AS 'Hash' FROM Ages WHERE MaxTurns = 0"/>
		<Row QueryId="TurnLimitCustomUpdates" Type="ConfigurationUpdates" SQL="SELECT 'Game' AS SourceGroup, 'StartAge' AS SourceKey, AgeType AS SourceValue, 'Game' AS TargetGroup, 'TurnLimitType' AS TargetKey, 'TURNLIMIT_CUSTOM' AS TargetValue, 1 AS 'Hash' FROM Ages WHERE MaxTurns > 0"/>
		<Row QueryId="MaxTurnUpdates" Type="ConfigurationUpdates" SQL="SELECT 'Game' AS SourceGroup, 'AGE' AS SourceKey, AgeType AS SourceValue, 'Game' AS TargetGroup, 'MaxTurns' AS TargetKey, MaxTurns AS TargetValue FROM Ages WHERE MaxTurns IS NOT NULL"/>
		<Row QueryId="VictoryUpdates" Type="ConfigurationUpdates" SQL="SELECT 'Game' As SourceGroup, 'AGE' AS SourceKey, AgeType AS SourceValue, 'Game' AS TargetGroup, VictoryType AS TargetKey, 1 AS TargetValue FROM Victories V, Ages WHERE V.EnabledByDefault = 1 AND V.Domain = Ages.VictoryDomain"/>
	</Queries>
	<MetaQueries>
		<!-- Live Event Meta-Queries-->
		<Row MetaQueryId="LiveEventDomainValueFilterQueries" MetaQueryType="QueryCriteria" SQL="SELECT LiveEventType || '_DVF' as QueryID, 'Game' as ConfigurationGroup, ConfigurationKey, 'Equals' as Operator, 1 as ConfigurationValue from LiveEvents"/>
		<Row MetaQueryId="LiveEventDomainValueFilterQueryCriteria" MetaQueryType="Queries" SQL="SELECT LiveEventType || '_DVF' as QueryID, 'DomainValueFilters' as Type, 'SELECT * FROM DomainValueFilters where Key1 = ''LiveEvent'' AND Key2 = '''|| LiveEventType || '''' as SQL from LiveEvents"/>
