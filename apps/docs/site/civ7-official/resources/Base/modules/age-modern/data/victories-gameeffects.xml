	<RequirementSet id="REQSET_VICTORY_MODERN_MILITARY">
		<Requirement type="REQUIREMENT_COLLECTION_ANY_MET">
			<Argument name="CollectionType">COLLECTION_TEAM_PLAYERS</Argument>
			<Argument name="RequirementSetId">REQSET_VICTORY_MODERN_MILITARY_OPERATION_IVY_COMPLETE</Argument>
		</Requirement>
	</RequirementSet>

	<RequirementSet id="REQSET_VICTORY_MODERN_MILITARY_OPERATION_IVY_COMPLETE">
		<Requirement type="REQUIREMENT_PLAYER_HAS_COMPLETED_PROJECT">
			<Argument name="ProjectType">PROJECT_OPERATION_IVY</Argument>
		</Requirement>
	</RequirementSet>

	<!-- Economic (Modern) Victory -->
	<RequirementSet id="REQSET_VICTORY_MODERN_ECONOMIC">
		<Requirement type="REQUIREMENT_COLLECTION_ANY_MET">
			<Argument name="CollectionType">COLLECTION_TEAM_PLAYERS</Argument>
			<Argument name="RequirementSetId">REQSET_VICTORY_MODERN_ECONOMIC_WORLD_BANKS_ESTABLISHED</Argument>
		</Requirement>
	</RequirementSet>

	<RequirementSet id="REQSET_VICTORY_MODERN_ECONOMIC_WORLD_BANKS_ESTABLISHED">
		<Requirement type="REQUIREMENT_PLAYER_HAS_VICTORY_POINTS_FOR_TYPE">
			<Argument name="GreatPersonIndividual">GREAT_PERSON_INDIVIDUAL_VICTORY_JOHN_MAYNARD_KEYNES</Argument>
