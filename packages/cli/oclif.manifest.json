        },
        "profile": {
          "description": "Profile key from config",
          "name": "profile",
          "required": false,
          "default": "default",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "engine": {
          "description": "Graphviz engine",
          "name": "engine",
          "default": "dot",
          "hasDynamicHelp": false,
          "multiple": false,
          "options": [
            "dot",
            "neato",
            "fdp",
            "sfdp",
            "circo",
            "twopi"
          ],
          "type": "option"
        },
        "format": {
          "description": "Output format",
