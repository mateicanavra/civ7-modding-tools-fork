      "pluginName": "@civ7-modding/cli",
      "pluginType": "core",
      "strict": true,
      "summary": "Render a Graphviz DOT file to an SVG (no external deps).",
      "enableJsonFlag": false,
      "isESM": false,
      "relativePath": [
        "dist",
        "commands",
        "render.js"
      ]
    },
    "slice": {
      "aliases": [],
      "args": {
        "manifest": {
          "description": "Path to manifest.txt (defaults to out/<seed>/manifest.txt)",
          "name": "manifest",
          "required": true
        }
      },
      "description": "\nReads a manifest.txt (one absolute path per line) and copies those files from a given root into a destination folder,\npreserving relative directory structure.\n",
      "examples": [
        "<%= config.bin %> slice ./out/<seed>/manifest.txt",
        "<%= config.bin %> slice --profile default ./out/<seed>/manifest.txt ./out/<seed>-slice"
      ],
      "flags": {
        "config": {
          "description": "Path to config file",
          "name": "config",
          "required": false,
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "profile": {
          "description": "Profile key from config",
          "name": "profile",
          "required": false,
          "default": "default",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "root": {
          "description": "Override root directory (XML source) if not using config",
          "name": "root",
          "required": false,
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "dest": {
          "description": "Override destination directory",
          "name": "dest",
          "required": false,
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        }
