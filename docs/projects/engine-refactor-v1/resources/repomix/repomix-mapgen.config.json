    "mods/mod-swooper-maps/src/**",
    "mods/mod-swooper-maps/*.json",
    "mods/mod-swooper-maps/test/**",
    "docs/projects/engine-refactor-v1/resources/spec/**",
    "scripts/lint/lint-domain-refactor-guardrails.sh"
  ],
  "ignore": {
    "patterns": [
      "docs/projects/engine-refactor-v1/resources/repomix/ecology-canonical-repomix.xml",
      "mods/**/dist/**",
      "mods/**/mod/**",
      "**/dist/**",
      "**/workflow/**",
